{
  "name": "cyrill-lehmann.ch",
  "version": "3.1.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/adabs-urdum/cyrill-lehmann.ch"
  },
  "main": "main.js",
  "scripts": {
    "build:setup": "mkdir -p dist/css && mkdir -p dist/js && mkdir -p assets/styles && mkdir -p assets/js && touch assets/styles/variables.scss && touch assets/styles/main.scss && touch assets/js/main.js",
    "build:cssdev": "node-sass assets/styles/main.scss --output-style expanded --source-map dist/css/styles.min.css.map --error-bell dist/css/styles.min.css",
    "build:cssprod": "node-sass assets/styles/main.scss --output-style compressed dist/css/styles.min.css",
    "build:jsprodOld": "webpack --mode production ./assets/js/main.js --output-path ./dist/js/",
    "build:jsprod": "esbuild ./assets/js/main.js --outfile=./dist/js/main.js --minify '--define:process.env.NODE_ENV=\"production\"' --bundle --loader:.js=jsx --target=es2015",
    "build:jsdev": "esbuild ./assets/js/main.js --outfile=./dist/js/main.js --sourcemap '--define:process.env.NODE_ENV=\"development\"' --bundle --loader:.js=jsx --target=es2016",
    "watch:cssdev": "onchange 'assets/styles/**/*.scss' -- npm run build:cssdev",
    "watch:jsdev": "onchange 'assets/js/**/*.js' -- npm run build:jsdev",
    "serve": "browser-sync start --proxy https://cyrill-lehmann.ch.test --files 'dist/**/*.css, dist/**/*.js' -w",
    "postinstall": "npm run build:setup",
    "eject": "react-scripts eject",
    "build": "npm run build:cssprod && npm run build:jsprodOld",
    "start": "parallelshell 'npm run watch:cssdev' 'npm run watch:jsdev' 'npm run serve'"
  },
  "author": "Cyrill Lehmann",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "latest",
    "@babel/plugin-proposal-class-properties": "latest",
    "@babel/plugin-transform-arrow-functions": "latest",
    "@babel/plugin-transform-async-to-generator": "latest",
    "@babel/plugin-transform-modules-commonjs": "latest",
    "@babel/preset-env": "latest",
    "@babel/preset-react": "latest",
    "axios": "latest",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "latest",
    "babylonjs": "latest",
    "babylonjs-loaders": "latest",
    "babylonjs-materials": "latest",
    "browser-sync": "latest",
    "esbuild-darwin-64": "latest",
    "node-sass": "latest",
    "onchange": "latest",
    "package.json": "latest",
    "parallelshell": "3.0.1",
    "preloader": "latest",
    "react": "latest",
    "react-dom": "latest",
    "webpack": "latest",
    "webpack-cli": "latest",
    "dotenv": "latest",
    "clean-webpack-plugin": "latest",
    "webpack-manifest-plugin": "latest",
    "friendly-errors-webpack-plugin": "latest",
    "mini-css-extract-plugin": "latest",
    "autoprefixer": "latest",
    "cssnano": "latest",
    "eslint-loader": "latest",
    "eslint": "latest"
  }
}
